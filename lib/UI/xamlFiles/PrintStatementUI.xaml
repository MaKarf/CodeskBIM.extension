<Window xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        Title="CodeskBIM Print Statement"
        WindowStartupLocation="CenterScreen"
        ShowInTaskbar="False"
        mc:Ignorable="d"
        SizeToContent="Height"
        HorizontalAlignment="Center">

    <StackPanel x:Name="panel" HorizontalAlignment="Left" Margin="5" Orientation="Vertical" VerticalAlignment="Top">

        <StackPanel>
            <TextBlock x:Name="print_statements"/>
        </StackPanel>

        <StackPanel HorizontalAlignment="Left" Margin="2">
            <TextBlock x:Name="error_statements" Foreground="Red"/>
        </StackPanel>

    </StackPanel>

</Window>
